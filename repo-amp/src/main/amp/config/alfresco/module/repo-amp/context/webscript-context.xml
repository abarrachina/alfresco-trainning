<?xml version='1.0' encoding='UTF-8'?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />
    <context:component-scan base-package="com.users.migratewebscript" />
 
    <import resource="service-context.xml"/>
    
    	<bean id="migrate-action" class="com.repo.action.executer.MigrateActionExecuter" parent="action-executer">
			<property name="serviceRegistry" ref="ServiceRegistry" />
			<property name="migrateService" ref="migrateServiceImpl" />
			<property name="personService" ref="PersonService" />
			<property name="searchService" ref="SearchService" />
	 	</bean>

		<bean id="webscript.users.migrateuser.post" class="com.users.migratewebscript.MigrateUser" parent="webscript">
			<property name="serviceRegistry" ref="ServiceRegistry" />
			<property name="personService" ref="PersonService" />
		</bean>
</beans>
